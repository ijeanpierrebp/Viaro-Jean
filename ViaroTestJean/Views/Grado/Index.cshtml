@model IEnumerable<Grado>

@{
    ViewData["Title"] = "Grado";
}
<a asp-action="Create" class="btn btn-primary">Nuevo</a>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>ProfesorId</th>
            <th>Nombre Profesor</th>
            <th colspan="2">Acciones</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.ProfesorId</td>
                <td>@item.NombreProfesor</td>
                <td>
                        <a asp-action="Update" asp-route-id="@item.Id" class="btn btn-secondary">Editar</a> |
                        <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Eliminar</a>
                </td>
            </tr>
        }
    </tbody>
</table>